{
  "name": "nail-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky && husky install",
    "lint": "eslint . --ext .ts,.vue",
    "docs:dev": "pnpm run register:components && vitepress dev docs",
    "docs:build": "pnpm run register:components && vitepress build docs --base /nailui-docs/",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "test": "vitest",
    "build:components": "node ./scripts/build.cjs"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.3",
    "@types/node": "^20.12.4",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "happy-dom": "^14.5.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitepress": "^1.0.1",
    "vitepress-theme-demoblock": "^3.0.7",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --cache --fix"
  }
}